{
  "menu-command": {
    "type": "entry",
    "default": "/usr/bin/eta-menu",
    "description": "Çalıştırılacak komut"
  },
  "use-icon": {
    "type": "checkbox",
    "default": true,
    "description": "Simge göster"
  },

  "icon-name": {
    "type": "iconfilechooser",
    "default": "start-here",
    "description": "Simge",
    "dependency": "use-icon"
  },
"icon-size": {
    "type": "spinbutton",
    "default": 32,
    "min": 16,
    "max": 96,
    "step": 1,
    "units": "px",
    "description": "Simge boyutu (px)",
    "dependency": "use-icon"
},

  "use-label": {
    "type": "checkbox",
    "default": true,
    "description": "Metin göster"
  },

  "label-text": {
    "type": "entry",
    "default": "Menu",
    "description": "Metin",
        "dependency": "use-label"
  },
 "overlay-key" : {
    "type" : "keybinding",
    "description" : "Keyboard shortcut to open and close the menu",
    "default" : "Super_L::Super_R"
 }

}

